<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<!---->
<struts>
	<package name="cart" extends="json-default">
		<action name="cart" class="cartAction">
			<result name="state" type="json">
				<param name="root">state</param>
			</result>
			<result name="myCart">shopping_cart.jsp</result>
			<result name="balance">balance.jsp</result>
		</action>
	</package>

</struts>
